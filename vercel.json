{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/history.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/history/(.*)",
      "dest": "/api/history.py"
    }
  ],
  "functions": {
    "api/webhook.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "TELEGRAM_TOKEN": "@telegram_token",
    "HF_API_TOKEN": "@hf_api_token",
    "BLOB_READ_WRITE_TOKEN": "@blob_read_write_token"
  }
}
